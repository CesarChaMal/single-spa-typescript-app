{
  "name": "@cesarchamal/single-spa-typescript-app",
  "version": "1.0.18",
  "description": "TypeScript microfrontend for Single-SPA demonstrating type safety",
  "main": "dist/single-spa-typescript-app.js",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack",
    "build:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode development",
    "build:prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --mode production",
    "start": "webpack serve --mode development --port 4209",
    "serve": "npm run build && npx http-server dist -p 4209 --cors",
    "lint": "eslint src --ext .ts --fix"
  },
  "devDependencies": {
    "@types/minimatch": "^3.0.5",
    "@types/node": "^8.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.22.1",
    "ts-loader": "^8.4.0",
    "typescript": "^4.9.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-brands-svg-icons": "5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "single-spa": "^5.9.0",
    "rxjs": "^6.4.0"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cesarchamal/single-spa-typescript-app.git"
  },
  "keywords": [
    "single-spa",
    "typescript",
    "npm",
    "webpack"
  ],
  "author": "Cesar Francisco Chavez Maldonado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cesarchamal/single-spa-typescript-app/issues"
  },
  "homepage": "https://github.com/cesarchamal/single-spa-typescript-app#readme"
}
